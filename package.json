  "bugs": {
    "url": "https://github.com/joerez/MERN-SPA-starter"
  },
  "homepage": "https://github.com/joerez/MERN-SPA-starter",
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.3",
    "concurrently": "^3.6.0",
    "cookie-session": "^2.0.0-beta.3",
    "express": "^4.16.3",
    "google-auth-library": "^1.6.1",
    "google-calendar": "^1.3.2",
    "googleapis": "^32.0.0",
    "localtunnel": "^1.9.0",
    "moment": "^2.22.2",
    "mongoose": "^5.2.2",
    "node-schedule": "^1.3.0",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
